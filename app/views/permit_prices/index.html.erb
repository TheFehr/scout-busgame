<p id="notice"><%= notice %></p>

<h1>Permit Prices</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Permit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permit_prices.each do |permit_price| %>
      <tr>
        <td><%= permit_price.price %></td>
        <td><%= permit_price.permit.name %></td>
        <td><%= link_to 'Show', permit_price %></td>
        <td><%= link_to 'Edit', edit_permit_price_path(permit_price) %></td>
        <td><%= link_to 'Destroy', permit_price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permit Price', new_permit_price_path %>
